    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/src/index.js",
            "args": [
              "--url", "http://google.com",
              "--feat-input-tag", "--feat-src-link", "--feat-form-handler", "--feat-invisible-iframe", "--feat-rewrite-statusbar", "--feat-disable-rightclick", "--feat-ahref-link", "--feat-popup-window", "--feat-favicon-link", "--feat-old-technologies", "--feat-missleading-link", "--feat-hostname-title",
              "--feat-hostname-title",
              "--output-values-string",
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "runtimeExecutable": "${env:NODE_BIN}"
        }
    ]
}
