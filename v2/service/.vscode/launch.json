{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/Debug/phishsvc",
            "args": ["--enable-training-data",
                "--td-url", "https://www.google.com",
                "--td-class-value", "0",
                "--enable-features",
                "--feat-ssl-subject",
                "--node-bin", "/home/xstaruch/.nvm/versions/node/v12.13.0/bin/node",
                "--html-script", "/home/xstaruch/master-thesis/v2/html_features_js/src/index.js"
            ],
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Debug"
        }
    ]
}
