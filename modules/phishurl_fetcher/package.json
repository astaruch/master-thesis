{
  "name": "phishurl_fetcher",
  "version": "1.0.0",
  "description": "This module is for fetching phishing URL from web services (Phishtank, OpenPhish, ...) and store them in a database.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "serve": "ts-node src/index.ts | pino-pretty",
    "start": "nodemon",
    "lint": "eslint --ext js,ts,jsx,tsx ./src",
    "fix-lint": "eslint --ext js,ts,jsx,tsx ./src --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm migration:run"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "author": "Andrej Staruch",
  "license": "ISC",
  "dependencies": {
    "pg": "^7.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
    "@strv/eslint-config-node": "^1.0.4",
    "@strv/eslint-config-typescript": "^1.0.4",
    "@types/node": "^11.13.6",
    "@types/pino": "^5.8.6",
    "csv-parse": "^4.4.1",
    "eslint": "^5.16.0",
    "install": "^0.12.2",
    "nodemon": "^1.18.11",
    "npm": "^6.9.0",
    "pino": "^5.12.2",
    "pino-pretty": "^2.6.1",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.4"
  }
